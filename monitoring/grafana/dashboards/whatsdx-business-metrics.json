{
  "dashboard": {
    "id": null,
    "title": "WhatsDeX - Business Metrics",
    "tags": ["whatsdex", "business", "analytics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Daily Active Users",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "count(count by (user_id) (increase(whatsdx_user_activity_total[24h])))",
            "legendFormat": "Daily Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Messages Per Hour",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(whatsdx_messages_received_total[1h]) * 3600)",
            "legendFormat": "Messages/Hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 3,
        "title": "Command Success Rate",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(whatsdx_commands_success_total[5m])) / sum(rate(whatsdx_commands_total[5m])) * 100",
            "legendFormat": "Success Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Revenue (Premium Users)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(whatsdx_premium_users_total) * 5",
            "legendFormat": "Monthly Revenue ($)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "green", "value": 500}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Message Volume Trend",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "sum(rate(whatsdx_messages_received_total[1h]))",
            "legendFormat": "Messages/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(whatsdx_messages_sent_total[1h]))",
            "legendFormat": "Responses/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Messages per second", "min": 0}
        ],
        "legend": {"show": true}
      },
      {
        "id": 6,
        "title": "User Growth",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "whatsdx_total_users",
            "legendFormat": "Total Users",
            "refId": "A"
          },
          {
            "expr": "increase(whatsdx_new_users_total[24h])",
            "legendFormat": "New Users (24h)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "User Count", "min": 0}
        ]
      },
      {
        "id": 7,
        "title": "Top Commands Usage",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "topk(10, sum by (command) (rate(whatsdx_commands_total[1h])))",
            "legendFormat": "{{command}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "list"
        }
      },
      {
        "id": 8,
        "title": "User Engagement by Hour",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 14},
        "targets": [
          {
            "expr": "sum by (hour) (rate(whatsdx_messages_received_total[1h]))",
            "legendFormat": "{{hour}}:00",
            "refId": "A"
          }
        ],
        "xAxis": {"show": true},
        "yAxis": {"show": true, "unit": "short"},
        "color": {"mode": "spectrum"}
      },
      {
        "id": 9,
        "title": "Error Rate by Command",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 14},
        "targets": [
          {
            "expr": "sum by (command) (rate(whatsdx_commands_failed_total[5m])) / sum by (command) (rate(whatsdx_commands_total[5m])) * 100",
            "legendFormat": "{{command}} error rate %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Error Rate %", "min": 0, "max": 100}
        ]
      },
      {
        "id": 10,
        "title": "API Usage by Feature",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "sum by (feature) (rate(whatsdx_api_requests_total[1h]))",
            "legendFormat": "{{feature}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "table", "placement": "right"}
        }
      },
      {
        "id": 11,
        "title": "Revenue Trend",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "sum(whatsdx_premium_users_total) * 5",
            "legendFormat": "Monthly Revenue",
            "refId": "A"
          },
          {
            "expr": "sum(increase(whatsdx_premium_subscriptions_total[24h])) * 5",
            "legendFormat": "Daily Revenue",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Revenue ($)", "min": 0}
        ]
      },
      {
        "id": 12,
        "title": "Feature Adoption Rate",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "sum by (feature) (rate(whatsdx_feature_usage_total[24h])) / sum(rate(whatsdx_messages_total[24h])) * 100",
            "legendFormat": "{{feature}} adoption %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "colorMode": "background",
          "orientation": "horizontal"
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "time_range",
          "type": "interval",
          "query": "1h,6h,12h,24h,7d,30d",
          "current": {"value": "24h", "text": "24h"}
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "changes(whatsdx_version_info[1h])",
          "iconColor": "green",
          "textFormat": "Deployment"
        }
      ]
    },
    "refresh": "1m",
    "schemaVersion": 27,
    "version": 0,
    "links": [
      {
        "title": "Technical Dashboard",
        "url": "/d/whatsdx-comprehensive/whatsdx-comprehensive-monitoring",
        "type": "dashboards"
      }
    ]
  }
}