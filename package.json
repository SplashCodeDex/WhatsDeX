{
  "name": "whatsdx",
  "version": "1.4.13-alpha.1",
  "description": "WhatsDeX - AI-Powered WhatsApp Bot SaaS Platform",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:saas": "PERSIST_SESSIONS=true WEB_QR_ENABLED=true PRINT_QR_IN_TERMINAL=false node index.js",
    "dev": "nodemon .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "seed": "node scripts/seed.js",
    "build": "npm run generate && npm run migrate",
    "e2e": "playwright test",
    "audit": "npm audit",
    "security": "npm audit --audit-level=moderate",
    "deploy:saas": "./deployment/deploy-smart.sh",
    "logs": "tail -f logs/*.log 2>/dev/null || echo 'No logs yet'",
    "backup": "node scripts/backup.js",
    "health": "curl -f http://localhost:3000/health || echo 'Service not ready'"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "ai",
    "saas",
    "chatbot",
    "automation",
    "gemini",
    "openai"
  ],
  "author": "WhatsDeX Team",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.8",
    "express": "^4.21.1",
    "socket.io": "^4.8.1",
    "redis": "^4.7.0",
    "@prisma/client": "^5.22.0",
    "prisma": "^5.22.0",
    "axios": "^1.7.7",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^10.0.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "chalk": "^4.1.2",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "sharp": "^0.33.5",
    "fluent-ffmpeg": "^2.1.3",
    "ytdl-core": "^4.11.5",
    "googleapis": "^144.0.0",
    "openai": "^4.69.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.14.0",
    "@playwright/test": "^1.48.2",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/whatsdx"
  },
  "bugs": {
    "url": "https://github.com/yourusername/whatsdx/issues"
  },
  "homepage": "https://github.com/yourusername/whatsdx#readme"
}