{
  "name": "whatsdex",
  "version": "1.4.13-alpha.1",
  "description": "Advanced WhatsApp Bot with AI Integration and Multi-Platform Support",
  "main": "index.js",
  "directories": {
    "commands": "commands",
    "events": "events",
    "tools": "tools"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "start": "node .",
    "start:prod": "node .",
    "start:pm2": "pm2 start ./index.js --name $(basename $(pwd))",
    "dev": "nodemon .",
    "dev:docker": "docker-compose up --build",
    "build": "npm run lint && npm test",
    "pre-commit": "npm run lint && npm test",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "docker:build": "docker build -t whatsdex .",
    "docker:run": "docker run -p 3000:3000 whatsdex",
    "launch": "node scripts/simple-launcher.js",
    "gui:install-mui": "cd gui && npm install @mui/material @emotion/react @emotion/styled",
    "gui:install-express-socketio": "cd gui && npm install express socket.io",
    "gui:install-electron": "cd gui && npm install electron --save-dev",
    "e2e": "playwright test",
    "security-audit": "npm audit --audit-level=high"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SplashCodeDex/WhatsDeX.git"
  },
  "keywords": [
    "whatsapp",
    "whatsapp-bot"
  ],
  "author": "CodeDeX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SplashCodeDex/WhatsDeX/issues"
  },
  "homepage": "https://github.com/SplashCodeDex/WhatsDeX#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^7.120.4",
    "@sentry/profiling-node": "^7.120.4",
    "@socket.io/redis-adapter": "^8.3.0",
    "@whiskeysockets/baileys": "^6.7.20",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cfonts": "^3.3.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "didyoumean": "^1.2.2",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.3.0",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "helmet": "^7.2.0",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "link-preview-js": "^3.1.0",
    "lucide-react": "^0.546.0",
    "magicui-next": "^0.1.24",
    "mime-types": "^2.1.35",
    "moment-timezone": "^0.6.0",
    "multer": "^1.4.5-lts.2",
    "pino": "^8.21.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "redis": "^4.7.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "vcard-creator": "^0.7.2",
    "wa-sticker-formatter": "^4.4.4",
    "winston": "^3.18.3",
    "yargs": "^18.0.0",
    "zod": "^4.1.12"
  },
