{
  "name": "whatsdx",
  "type": "module",
  "version": "1.4.13-alpha.1",
  "description": "WhatsDeX - AI-Powered WhatsApp Bot SaaS Platform",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:smart": "node backend/smart-start.js",
    "start:dev": "nodemon --quiet --signal SIGTERM --delay 300ms --watch src --watch routes --ext js,mjs,cjs index.js",
    "start:web": "cd frontend && npm run dev",
    "dev:all": "node scripts/start-multitenant.js",
    "worker": "node backend/worker.js",
    "dev": "nodemon .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "generate": "npx prisma generate --schema backend/prisma/schema.prisma",
    "migrate": "npx prisma migrate deploy --schema backend/prisma/schema.prisma",
    "migrate:dev": "npx prisma migrate dev --schema backend/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema backend/prisma/schema.prisma",
    "seed": "node scripts/seed.js",
    "build": "npm run generate",
    "e2e": "playwright test",
    "audit": "npm audit",
    "security": "npm audit --audit-level=moderate",
    "deploy:saas": "./deployment/deploy.sh",
    "logs": "tail -f logs/*.log 2>/dev/null || echo 'No logs yet'",
    "backup": "node scripts/backup.js",
    "health": "curl -f http://localhost:3001/health || echo 'Service not ready'",
    "prepare": "husky"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "ai",
    "saas",
    "chatbot",
    "automation",
    "gemini",
    "openai"
  ],
  "author": "Codedex <https://github.com/splashcodedex>",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-label": "^2.1.8",
    "@whiskeysockets/baileys": "^6.7.21",
    "aki-api": "^7.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "chess.js": "^1.4.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "didyoumean": "^1.2.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^166.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "openai": "^6.8.1",
    "pino": "^10.1.0",
    "prisma": "^6.19.0",
    "qr-image": "^3.2.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "qrcode.react": "^4.2.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.0",
    "uuid": "^13.0.0",
    "wa-sticker-formatter": "^4.4.4",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@playwright/test": "^1.48.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/splashcodedex/whatsdex.git"
  },
  "bugs": {
