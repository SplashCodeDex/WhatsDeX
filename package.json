{
  "name": "whatsdex",
  "version": "1.4.13-alpha.1",
  "description": "Useless WhatsApp Bot",
  "main": "index.js",
  "directories": {
    "commands": "commands",
    "events": "events",
    "tools": "tools"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "start": "node .",
    "start:pm2": "pm2 start ./index.js --name $(basename $(pwd))",
    "dev": "nodemon .",
    "build": "npm run lint && npm test",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "docker:build": "docker build -t whatsdex .",
    "docker:run": "docker run -p 3000:3000 whatsdex",
    "launch": "node scripts/ultra-smart-launcher.js",
    "launch:bot-only": "node scripts/ultra-smart-launcher.js --bot-only",
    "launch:admin-only": "node scripts/ultra-smart-launcher.js --admin-only",
    "launch:qr-only": "node scripts/ultra-smart-launcher.js --qr-only",
    "launch:pairing-only": "node scripts/ultra-smart-launcher.js --pairing-only",
    "analytics": "node scripts/analytics-dashboard.js",
    "analytics-api": "node scripts/analytics-server.js",
    "health-check": "node scripts/health-check.js",
    "session-manager": "node scripts/session-manager.js",
    "diagnostics": "node scripts/diagnostics.js",
    "auth-debug": "node scripts/auth-debug.js",
    "session-debug": "node scripts/session-debug.js",
    "performance-monitor": "node scripts/performance-monitor.js",
    "gui:install-mui": "cd gui && npm install @mui/material @emotion/react @emotion/styled",
    "gui:install-express-socketio": "cd gui && npm install express socket.io",
    "gui:install-electron": "cd gui && npm install electron --save-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SplashCodeDex/WhatsDeX.git"
  },
  "keywords": [
    "whatsapp",
    "whatsapp-bot"
  ],
  "author": "CodeDeX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SplashCodeDex/WhatsDeX/issues"
  },
  "homepage": "https://github.com/SplashCodeDex/WhatsDeX#readme",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^5.7.1",
    "@sentry/node": "^7.109.0",
    "@sentry/profiling-node": "^7.109.0",
    "@whiskeysockets/baileys": "^6.7.5",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cfonts": "^3.3.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "didyoumean": "^1.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.0",
    "guaranteed_security": "^1.0.0",
    "helmet": "^7.2.0",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "link-preview-js": "^3.1.0",
    "mime-types": "^2.1.35",
    "moment-timezone": "^0.6.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "pino": "^8.21.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "rate-limiter-flexible": "^3.0.4",
    "socket.io": "^4.8.1",
    "stripe": "^14.14.0",
    "vcard-creator": "^0.5.0",
    "wa-sticker-formatter": "^4.4.4",
    "winston": "^3.17.0",
    "yargs": "^18.0.0",
    "zod": "^4.1.5"
  },
  "optionalDependencies": {
    "baileys-firebase": "^1.0.1",
    "baileys-mongodb": "^1.0.9",
    "baileys-mysql": "^1.0.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/yargs": "^17.0.33",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3"
  }
}
