{
  "name": "whatsdex-root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev:frontend": "npm run dev -w frontend",
    "dev:backend": "npm run dev -w backend",
    "build:frontend": "npm run build -w frontend",
    "lint:frontend": "npm run lint -w frontend",
    "lint:backend": "npm run lint -w backend",
    "test:frontend": "npm run test -w frontend",
    "test:backend": "npm run test -w backend",
    "test:all": "npm run test -w backend && npm run test -w frontend",
    "build:backend": "npm run build -w backend"
  },
  "devDependencies": {
    "lightningcss": "^1.30.2"
  },
  "overrides": {
    "fast-xml-parser": "^5.4.1",
    "axios": "^1.13.5",
    "sharp": "^0.34.5"
  },
  "dependencies": {
    "codedex-api-manager": "file:../MyTemplates"
  }
}
