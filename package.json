{
  "name": "whatsdex",
  "version": "1.4.13-alpha.1",
  "description": "Advanced WhatsApp Bot with AI Integration and Multi-Platform Support",
  "main": "index.js",
  "directories": {
    "commands": "commands",
    "events": "events",
    "tools": "tools"
  },
  "scripts": {
    "test": "cross-env DATABASE_URL=\"file:./dev.db\" jest",
    "lint": "eslint",
    "start": "node .",
    "start:prod": "node .",
    "start:pm2": "pm2 start ./index.js --name $(basename $(pwd))",
    "dev": "nodemon .",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "dev:frontend": "cd web && npm run dev",
    "dev:docker": "docker-compose up --build",
    "build": "npm run lint && npm test",
    "build:frontend": "cd web && npm run build",
    "build:full": "npm run build && npm run build:frontend",
    "pre-commit": "npm run lint && npm test",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "docker:build": "docker build -t whatsdex .",
    "docker:run": "docker run -p 3000:3000 whatsdex",
    "docker:build:full": "docker-compose build",
    "docker:run:full": "docker-compose up",
    "install:all": "npm install && cd web && npm install",
    "setup:frontend": "cd web && npm install",
    "launch": "node scripts/simple-launcher.js",
    "e2e": "playwright test",
    "security-audit": "npm audit --audit-level=high",
    "seed": "cross-env DATABASE_URL=\"file:./dev.db\" node scripts/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SplashCodeDex/WhatsDeX.git"
  },
  "keywords": [
    "whatsapp",
    "whatsapp-bot"
  ],
  "author": "CodeDeX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SplashCodeDex/WhatsDeX/issues"
  },
  "homepage": "https://github.com/SplashCodeDex/WhatsDeX#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@prisma/client": "^6.18.0",
    "@sentry/node": "^7.120.4",
    "@socket.io/redis-adapter": "^8.3.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@whiskeysockets/baileys": "^7.0.0-rc.6",
    "aki-api": "^7.0.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bullmq": "^5.62.1",
    "cfonts": "^3.3.1",
    "chess.js": "^1.4.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "didyoumean": "^1.2.2",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.3.0",
    "form-data": "^4.0.4",
    "guaranteed_security": "^1.0.0",
    "helmet": "^7.2.0",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "link-preview-js": "^3.1.0",
    "magicui-next": "^0.1.24",
    "mime-types": "^2.1.35",
    "moment-timezone": "^0.6.0",
    "multer": "^1.4.5-lts.2",
    "next": "^16.0.1",
    "node-cache": "^5.1.2",
    "pino": "^8.21.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redis": "^4.7.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "vcard-creator": "^0.7.2",
    "wa-sticker-formatter": "^4.4.4",
    "winston": "^3.18.3",
    "yargs": "^18.0.0",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "baileys-firebase": "^1.0.1",
    "baileys-mongodb": "^1.0.9",
    "baileys-mysql": "^1.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.1.1",
    "nodemon": "^3.0.2",
    "prisma": "^6.18.0",
    "supertest": "^6.3.3"
  }
}
